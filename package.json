  "version": "0.0.14",
  "main": "src/index.js",
  "browser": "dist/index.js",
  "author": "Mikael Jorhult",
  "license": "MIT",
  "keywords": [
    "gantt",
    "chart",
    "timeline",
    "schedule",
    "calendar",
